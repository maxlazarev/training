			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        RecId jourRefRecId;

        if (element.args().caller() && element.args().caller() is FormRun)
        {
            FormRun callerFormRun = element.args().caller();
            if (callerFormRun.name() == formStr(SalesEditLines) && callerFormRun.args().caller())
            {
                SalesFormLetter salesFormLetter = callerFormRun.args().caller();
                
                if (salesFormLetter)
                {
                    jourRefRecId = salesFormLetter.FINParmProformaInvoiceJourRecId();
                }
            }
        }
           
        if (jourRefRecId)
        {
            FINCustInvoiceJourProForma custInvoiceJourProForma;
            select firstonly InvoiceId from custInvoiceJourProForma
            where custInvoiceJourProForma.RecId == jourRefRecId;          
                    
            InvoiceId invoiceIdlocal = custInvoiceJourProForma.InvoiceId;
            helperClass = FINRentalInvoiceDetailHelper::getRentalInvoiceDetailsForProforma(invoiceIdlocal, element.args().record(), element.args().parm() == "@DNR435");
        }
        else
        {
            helperClass = FINRentalInvoiceDetailHelper::getRentalInvoiceDetails(element.args().record(), element.args().parm() == "@DNR435");
        }
        super();

        this.updateDesign();

        OrderId.text(helperClass.orderId);
        InvoiceId.text(helperClass.invoiceId);
        InvoiceId.visible(helperClass.invoiceId != '');
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    public void updateDesign()
    {
        PurchLine purchLine = helperClass.rentalLine as PurchLine;
        UnitOfMeasure rateCode = UnitOfMeasure::find(helperClass.rentalLine.PriceTimeUnit);
      
        //Rental group
        boolean isRentalVisible = rateCode.FINInvoiceRentalEngine && FINInvoiceCalculatorEngineSetup::isEngineActive(FINRentalAccountType::Price);
        RentalBaseValues.visible(isRentalVisible);
        RentalGrid.visible(isRentalVisible);
        RentalTotalValues.visible(isRentalVisible);

        DeliveryGroup.visible(rateCode.FINInvoiceTransportEngine && FINInvoiceCalculatorEngineSetup::isEngineActive(FINRentalAccountType::DeliveryTransport));
        ReturnGroup.visible(rateCode.FINInvoiceTransportEngine && FINInvoiceCalculatorEngineSetup::isEngineActive(FINRentalAccountType::DeliveryTransport));
        CostGroup.visible(!purchLine && rateCode.FINInvoiceCostPriceEngine && FINInvoiceCalculatorEngineSetup::isEngineActive(FINRentalAccountType::CostPrice));
        ProjGroup.visible(rateCode.FINInvoiceProjectEngine && FINInvoiceCalculatorEngineSetup::isEngineActive(FINRentalAccountType::ProjectHours));
        ManualComponentGroup.visible(rateCode.FINInvoiceManualEngine && FINInvoiceCalculatorEngineSetup::isEngineActive(FINRentalAccountType::Manual));
    }

]]></Source>
