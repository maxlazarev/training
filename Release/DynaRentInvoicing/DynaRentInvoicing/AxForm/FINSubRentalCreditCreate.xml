				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            if (creditFormClass.orderId())
            {
                rangeOrderId.value(queryValue(creditFormClass.orderId()));
            }
        
            if (creditFormClass.invoiceAccount())
            {
                rangeInvoiceAccount.value(queryValue(creditFormClass.invoiceAccount()));
            }
        
            if (creditFormClass.invoiceId())
            {
                rangeInvoiceId.value(queryValue(creditFormClass.invoiceId()));
            }
        
            super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        void init()
        {
            QueryBuildDataSource queryBuildDataSource;
        
            super();
        
            queryBuildDataSource = this.query().dataSourceTable(this.table());
            queryBuildDataSource.clearDynalinks();
        
            rangeOrderId = queryBuildDataSource.addRange(fieldNum(VendInvoiceJour, PurchId));
            rangeInvoiceAccount = queryBuildDataSource.addRange(fieldNum(VendInvoiceJour, InvoiceAccount));
            rangeInvoiceId = queryBuildDataSource.addRange(fieldNum(VendInvoiceJour, InvoiceId));
        
            creditFormClass.extendQuery(queryBuildDataSource);
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
        
            ApproveButton.enabled(!FINCreditLine::canCreditJourBeApproved(VendInvoiceJour.TableId, VendInvoiceJour.RecId));
            RejectButton.enabled(!FINCreditLine::canCreditJourBeRejected(VendInvoiceJour.TableId, VendInvoiceJour.RecId));
            buttonUpdateInvoice.enabled(FINCreditLine::canCreditJourBePosted(VendInvoiceJour.TableId, VendInvoiceJour.RecId));

            return ret;
        }

]]></Source>
