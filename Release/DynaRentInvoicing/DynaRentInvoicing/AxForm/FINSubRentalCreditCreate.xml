			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (PurchParameters::find().ChangeRequestEnabled && element.args().dataset() == tableNum(PurchTable))
        {
            PurchTable purchTable = element.args().record();
            if (purchTable && purchTable.DocumentState != VersioningDocumentState::Draft)
            {
                throw error("@DNR8491");
            }
        }
    
        creditFormClass = FINCreditForm::construct(SalesPurch::Purch);
        creditFormClass.initCallerBuffer(element.args().record());
        creditManager = FINCreditManager::construct(creditFormClass.salesPurch());
    
        super();

        if (!element.args().record())
        {
            FINCreditAddDropDialog.visible(false);
            FINCreditEditDropDialog.visible(false);
            ApproveButton.visible(true);
            RejectButton.visible(true);
        }
    
        creditFormClass.parmCreditFormRun(element);
    }

]]></Source>
			</Method>
