				<Source><![CDATA[
    /// <summary>
    /// Enable or disable relations fields depending on the ItemCode field.
    /// </summary>
    /// <param name="_invoicePosting">
    /// The current FINInvoicePosting table record to enable or disable form fields for.
    /// </param>
    void setRelationsEnabled(FINInvoicePosting _invoicePosting)
    {
        boolean allowEdit;
        FormDataObject formDataObject;

        if (FormDataUtil::isFormDataSource(_invoicePosting))
        {
            allowEdit = _invoicePosting.isItemRelationFieldRequired();
            formDataObject = FormDataUtil::getFormDataSource(_invoicePosting).object(fieldNum(FINInvoicePosting, ItemRelation));
            formDataObject.allowEdit(allowEdit);
            formDataObject.skip(!allowEdit);  
            formDataObject.mandatory(allowEdit);

            allowEdit = _invoicePosting.isCustVendAccountRelationFieldRequired();
            formDataObject = FormDataUtil::getFormDataSource(_invoicePosting).object(fieldNum(FINInvoicePosting, CustVendRelation));
            formDataObject.allowEdit(allowEdit);
            formDataObject.skip(!allowEdit);
            formDataObject.mandatory(allowEdit);
        }
    }

]]></Source>
