				<Name>setTabPage</Name>
				<Source><![CDATA[
    void setTabPage()
    {
        switch (element.args().dataset())
        {
            case tableNum(CustTable),tableNum(CustGroup):
                ctrlTabPurchSubRental.visible(false); 
                ctrlTabSalesRental.pageActivated();
                break;
            case tableNum(VendTable),tableNum(VendGroup):
                ctrlTabSalesRental.visible(false);
                ctrlTabSalesSubRental.visible(false);                    
                ctrlTabPurchSubRental.pageActivated();
                break;                
        }        
    }

]]></Source>
