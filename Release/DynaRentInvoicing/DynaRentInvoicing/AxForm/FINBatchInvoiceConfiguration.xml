					<Method>
						<Name>displayInvoiceCheckIcon</Name>
						<Source><![CDATA[
        [SysClientCacheDataMethodAttribute(true)]
        public display container displayInvoiceCheckIcon(FINBatchInvoiceConfiguration _batchInvoiceConfiguration)
        {
            ImageReference imageReference;
            container imageContainer;
            FINBatchInvoiceAuditTrail auditTrail;
            FINBatchInvoiceCheckLog invoiceCheckLog;

            select firstonly auditTrail
            order by CreationDate desc
            where auditTrail.ConfigurationId == _batchInvoiceConfiguration.ConfigurationId
                outer join RecId from invoiceCheckLog
                where invoiceCheckLog.AuditTrailRecId == auditTrail.RecId
                &&    invoiceCheckLog.Status == FININvoiceCheckStatus::Open;

            if (auditTrail.RecId)
            {
                if (auditTrail.InvoiceCheckDone)
                {
                    if (invoiceCheckLog)
                    {
                        imageReference = ImageReference::constructForSymbol(ImageReferenceSymbol::RedSquare);
                    }
                    else
                    {
                        imageReference = ImageReference::constructForSymbol(ImageReferenceSymbol::GreenSquare);
                    }
                }
                else
                {
                    imageReference = ImageReference::constructForSymbol(ImageReferenceSymbol::YellowSquare);
                }
            }

            if (imageReference)
            {
                imageContainer = imageReference.pack();
            }

            return imageContainer;
        }

]]></Source>
