			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class FINPurchTotals extends FormRun
{
    PurchTotals             purchTotals;
    PurchTable              callingPurchTable;
    PurchUpdate             purchUpdate;
    PurchtotalsForm         totalsForm;
    Common                  callingTable;
    int                     specQty;
    Object                  callerObject;
    Set                     selectedRecId;
    FINPurchTotals          finPurchTotals;

}
]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        super();

        if (finPurchTotals)
        {
            finPurchTotals.cleanupPrecalculatedData();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>calcTotals</Name>
				<Source><![CDATA[
    void  calcTotals()
    {
        using (DNRTradeTotalsDisposableContext context = new DNRTradeTotalsDisposableContext())
        {
            context.DNRParmCalledFromForm(true); // The value is used in /Classes/DNRPurchTotals_SalesClass_Extension/Methods/construct
            context.DNRParmSelectedRecId(selectedRecId);
            totalsForm.calctotals();
        }        
        
        totalsForm.doPack();
        element.setDisplayFields();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    [SuppressBPWarningAttribute("BPCheckInvalidInitFormMethodWarning", "Validation of caller must be performed before invoking super()")]
    void  init()
    {
        xArgs   args;

        args = element.args();

        finPurchTotals = element.args().caller() as FINPurchTotals;

        super();

        selectedRecId = new Set(Types::Int64);

        if (args.dataset() == tableNum(PurchLine))
        {
            PurchLine purchLine = args.record() as PurchLine;
            selectedRecId.add(purchLine.RecId);
            callingTable = purchLine.purchTable();
        }
        else
        {
            callingTable = args.record();
        }

        specQty      = args.parmEnum();
                
        totalsForm   = PurchtotalsForm::newPurchTotalsForm(callingTable, specQty);
        totalsForm.doUnPack();

        if (finPurchTotals)
        {
            TotalsTabPage.caption("@DNR10206");
        }
        else
        // Called from PurchRFQCompare form it is unclear which vendor reply the totals are showing for, so display this information on the tab title
        if (callingTable.TableId == tableNum(PurchRFQReplyTable) && args.parm() != '')
        {
            TotalsTabPage.caption(totalsForm.totalsTabCaption() + ' - ' + strFmt("@SYS82246", args.parm()));
        }
        else
        {
            TotalsTabPage.caption(totalsForm.totalsTabCaption());
        }

        currencyCode.allowEdit(totalsForm.allowEditCurrencyCode());

        updateSelection.enumType(totalsForm.updateSelectionType());
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();

        updateselection.visible(totalsForm.updateSelection());
        updateSelection.selection(totalsForm.updateSelectionValue());
        qty.visible(totalsForm.qty());
        weight.visible(totalsForm.weight());
        volume.visible(totalsForm.volume());
        cashDisc.visible(totalsForm.cashDisc());
        vendCreditMax.visible(totalsForm.vendCreditMax());
        vendFreeValue.visible(totalsForm.vendFreeValue());
        prepay.visible(totalsForm.prepayVisible());
    }

]]></Source>
			</Method>
			<Method>
				<Name>setDisplayFields</Name>
				<Source><![CDATA[
    void  setDisplayFields()
    {
        qty.realValue               (totalsForm.qtyValue());
        cwQty.realValue             (totalsForm.pdsCWQtyValue());
        weight.realValue            (totalsForm.weightValue());
        volume.realValue            (totalsForm.volumeValue());
        currencyCode.text           (totalsForm.currencyCodeValue());
        exchRate.realValue          (totalsForm.exchRateValue());
        vendCreditMax.realValue     (totalsForm.vendCreditMaxValue());
        cashDisc.realValue          (totalsForm.cashDiscValue());
        vendFreeValue.realValue     (totalsForm.vendFreeValueValue());
        lineDisc.realValue          (totalsForm.lineDiscValue());
        sumLines.realValue          (totalsForm.sumLinesValue());
        endDisc.realValue           (totalsForm.endDiscValue());
        sumMarkup.realValue         (totalsForm.sumMarkUpValue());
        sumTax.realValue            (totalsForm.sumTaxValue());
        invoiceRoundOff.realValue   (totalsForm.invoiceRoundOffValue());
        invoiceAmount.realValue     (totalsForm.invoiceAmountValue());

        creditPrepayDataGroup.visible(totalsForm.showCreditPrepayDataGroup());
        endDisc.visible             (totalsForm.endDiscValueVisible());

        reqLineCount.visible        (totalsForm.lineCountVisible());
        lineCount.value             (totalsForm.lineCount());

        prepayLimit.realValue       (totalsForm.purchPrepayLimit());
        prepayAvailable.realValue   (totalsForm.purchPrepayAvailable());
        taxWithhold_TH.realValue    (totalsForm.sumTaxWithhold_THValue());

        // <GBR>
        if (BrazilParameters::isEnabled())
        {
            sumMarkup.realValue             (totalsForm.markupTotalAmount_BR());

            salesTaxCurrencyCode_BR.text       (totalsForm.salesTaxCurrencyCodeValue_BR());
            icmsAmount_BR.realValue            (totalsForm.purchICMSTotal_BR());
            ICMSSTAmount_BR.realValue          (totalsForm.purchICMSSTTotal_BR());
            icmsDiffAmount_BR.realValue        (totalsForm.purchICMSDiffTotal_BR());
            ipiAmount_BR.realValue             (totalsForm.purchIPITotal_BR());
            pisAmount_BR.realValue             (totalsForm.purchPISTotal_BR());
            cofinsAmount_BR.realValue          (totalsForm.purchCOFINSTotal_BR());
            importTaxAmount_BR.realValue       (totalsForm.purchImportTaxTotal_BR());
            irrfAmount_BR.realValue            (totalsForm.purchIRRFTotal_BR());
            issAmount_BR.realValue             (totalsForm.purchISSTotal_BR());
            inssAmount_BR.realValue            (totalsForm.purchINSSTotal_BR());
            csllAmount_BR.realValue            (totalsForm.purchCSLLTotal_BR());
            INSSRetainedAmount_BR.realValue    (totalsForm.purchINSSRetainedTotal_BR());

            FreightAmount_BR.realValue         (totalsForm.markupFreightAmountMST_BR());
            InsuranceAmount_BR.realValue       (totalsForm.markupInsuranceAmountMST_BR());
            OtherChargesAmount_BR.realValue    (totalsForm.markupOthersAmountMST_BR());
            SiscomexAmount_BR.realValue        (totalsForm.markupSiscomexAmountMST_BR());
        }
        // </GBR>
    }

]]></Source>
