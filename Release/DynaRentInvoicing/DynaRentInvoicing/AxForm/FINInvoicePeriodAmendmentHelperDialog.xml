			<Method>
				<Name>validate</Name>
				<Source><![CDATA[
    public boolean validate()
    {
        boolean ret = true;

        // reason code is mandatory
        if (!ReasonCode.valueStr())
        {
            ret = checkFailed(strFmt("@SYS84378", "@SYS63086"));
        }

        return ret;
    }

]]></Source>
