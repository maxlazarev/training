			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        if (element.validate())
        {
            super();
    
            // Pass values from dialog to helper class
            callerClass.parmReasonCode(ReasonCode.valueStr());
            callerClass.parmReasonText(ReasonText.valueStr());
        }
    }

]]></Source>
