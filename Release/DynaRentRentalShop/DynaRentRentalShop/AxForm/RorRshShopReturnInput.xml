				<Name>refreshInventDimDatasource</Name>
				<Source><![CDATA[
    private void refreshInventDimDatasource()
    {
        InventDim.data(tmpRentalShopTrans.inventDim());
        InventDim_DS.setCurrent();
    }

]]></Source>
			</Method>
