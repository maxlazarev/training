        public void write()
        {
            boolean isNewRecord = (TmpRentalShopTrans.RecId == 0);    

            super();

            if (!isNewRecord)
            {
                //Delete previous log if updating exiting record.
                RorRshShopTransLog::deleteLog(TmpRentalShopTrans.TableId, TmpRentalShopTrans.RecId);
            }

            TmpRentalShopTrans.runLogBasedValidation();
        }

