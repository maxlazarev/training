			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    private void updateDesign(InventDimFormDesignUpdate _mode)
    {
        boolean emptyLine;

        switch (_mode)
        {
            case InventDimFormDesignUpdate::Init:
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup = RORRshInventDimCtrl_Frm_Editable::newFromFormItem(element, TmpRentalShopTrans.ItemId);
                }
                // Fall through

            case InventDimFormDesignUpdate::Active,
                 InventDimFormDesignUpdate::FieldChange:
                emptyLine = TmpRentalShopTrans.ItemId == '';
                inventoryDimensionsGrid.allowEdit(!emptyLine);
                inventDimFormSetup.formActiveSetup(InventDimGroupSetup::newItemId(TmpRentalShopTrans.ItemId));
                inventDimFormSetup.formSetControls(_mode == InventDimFormDesignUpdate::Active);
                
                break;

            default :
                throw error(strFmt("@SYS54195",funcName()));
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateRentalShopTransDesign</Name>
				<Source><![CDATA[
    private void updateRentalShopTransDesign()
    {
        boolean returnOnlyGood = (TmpRentalShopTrans.TradeType == RorRshShopTradeType::SalesPurch);

        TmpRentalShopTrans_ds.object(fieldNum(RorRshTmpShopTrans, QtyReturnDamaged)).allowEdit(!returnOnlyGood);
        TmpRentalShopTrans_ds.object(fieldNum(RorRshTmpShopTrans, QtyReturnScrapped)).allowEdit(!returnOnlyGood);
        TmpRentalShopTrans_ds.object(fieldNum(RorRshTmpShopTrans, QtyReturnSurplus)).allowEdit(!returnOnlyGood);
    }

]]></Source>
			</Method>
