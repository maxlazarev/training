				<Name>modifyInventDimField</Name>
				<Source><![CDATA[
    public void modifyInventDimField(FieldId _fieldId)
    {
        if (TmpRentalShopTrans.modifyInventDim(inventDim, _fieldId))
        {
            element.refreshInventDimDatasource();
        }
    }

]]></Source>
