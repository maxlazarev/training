		<Methods>
			<Method>
				<Name>postLoad</Name>
				<Source><![CDATA[
    /// <summary>
    /// post load method
    /// </summary>
    public void postLoad()
    {
        super();

        InventTransOrigin inventTransOrigin;
        InventTrans inventTrans;

        select firstOnly RecId from inventTransOrigin where inventTransOrigin.InventTransId == this.InventTransId;

        select firstOnly BOBPckSlpUpdDateTime from inventTrans
            order by inventTrans.ModifiedDateTime desc
                where inventTrans.inventTransOrigin == inventTransOrigin.RecId
                    && inventTrans.BOBPckSlpUpdDateTime && inventTrans.PackingSlipId == this.DocumentId;

        this.BOBPckSlpUpdDateTime = inventTrans.BOBPckSlpUpdDateTime;

        if (appl.ttsLevel() > 0 && RORParameters::find().DNRCPAsyncUpdate)
        {
            DNRCPEntityAsyncCaller::scheduleUpdate('DNRCPRentalLogisticalDatesPackingSlipEntity' , this);  // since it is not possible to adjust posted invoice we can call DW async even for insert
        }
    }

]]></Source>
			</Method>
