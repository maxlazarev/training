			<Method>
				<Name>mapEntityToDataSource</Name>
				<Source><![CDATA[
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (_dataSourceCtx.name() == dataEntityDataSourceStr(DNRBusinessObjectAssignmentEntity, SalesLine))
        {
            SalesLine       salesLine = _dataSourceCtx.getBuffer();
            InventDim       inventDim;

            salesLine.BOBItemIdRequested        = salesLine.ItemId;

            switch (_entityCtx.getDatabaseOperation())
            {
                case DataEntityDatabaseOperation::Update:
                    // Set the correct inventDimId on the table buffer.
                    inventDim = salesLine.inventDim();
                    inventDim.inventSerialId   = this.inventSerialId   ? this.inventSerialId   : inventDim.inventSerialId;
                    inventDim.InventSiteId     = this.InventSiteId     ? this.InventSiteId     : inventDim.InventSiteId;
                    inventDim.InventLocationId = this.InventLocationId ? this.InventLocationId : inventDim.InventLocationId;
                    inventDim.wMSLocationId    = this.wMSLocationId    ? this.wMSLocationId    : inventDim.wMSLocationId;
                    salesLine.InventDimId      = InventDim::findOrCreate(inventDim).inventDimId;
                    break;
            }
        }
        
        super(_entityCtx, _dataSourceCtx);
    }

]]></Source>
			</Method>
			<Method>
				<Name>findEntityDataSource</Name>
				<Source><![CDATA[
    public Common findEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        Common ret;
        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(DNRBusinessObjectAssignmentEntity, InventDim):
                DataEntityDataSourceRuntimeContext lineCtx = _entityCtx.getRuntimeContextByName(dataEntityDataSourceStr(DNRBusinessObjectAssignmentEntity, SalesLine));
                SalesLine line = lineCtx.getBuffer();

                ret = InventDim::find(line.InventDimId);
                break;

            default:
                ret = super(_entityCtx, _dataSourceCtx);
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>mapDataSourceToEntity</Name>
				<Source><![CDATA[
    public void mapDataSourceToEntity(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (_dataSourceCtx.name() == dataEntityDataSourceStr(DNRBusinessObjectAssignmentEntity, SalesLine))
        {
            SalesLine salesLine = _dataSourceCtx.getBuffer();
            this.InventInventoryDimensionEntityFieldsMapping::initFromInventDim(salesLine.inventDim());
        }

        super(_entityCtx, _dataSourceCtx);
    }

]]></Source>
