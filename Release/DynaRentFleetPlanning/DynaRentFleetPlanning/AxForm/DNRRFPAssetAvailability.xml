						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret = super();
    
            FilterWarehouseId.text('');
            Query query = locationQueryRun.query();
            QueryBuildDataSource qbds = query.dataSourceTable(tableNum(InventLocation));
            
            
            qbds.clearRange(fieldNum(InventLocation, InventSiteId));
            if (this.valueStr())
            {
                QueryBuildRange qbr = SysQuery::findOrCreateRange(qbds, fieldNum(InventLocation, InventSiteId));
                qbr.value(SysQuery::value(this.valueStr()));
            }
            msCtrlLocation.refreshQueryRun(locationQueryRun);
            return ret;
        }

]]></Source>
