				<Name>updateFieldVisiblity</Name>
				<Source><![CDATA[
    /// <summary>
    /// Make the fields invisible based on setup
    /// </summary> 
    private void updateFieldVisiblity()
    {
        Map           fieldIdLabelMap = businessObjectData.getFieldIdLabelStrMap();
        MapEnumerator fieldIdLabelMapEnum = fieldIdLabelMap.getEnumerator();

        while (fieldIdLabelMapEnum.moveNext())
        {
            String20       frmCtrlName = fieldIdLabelMapEnum.currentKey();
            FormIntControl frmIntCtrl  = element.control(element.controlId(frmCtrlName)) as FormIntControl;

            frmIntCtrl.label(conPeek(fieldIdLabelMapEnum.currentValue(), 1));
            frmIntCtrl.helpText(conPeek(fieldIdLabelMapEnum.currentValue(), 2));
            frmIntCtrl.visible(true);
        }
    }

]]></Source>
