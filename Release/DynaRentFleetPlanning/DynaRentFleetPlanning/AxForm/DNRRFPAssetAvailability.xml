			<Method>
				<Name>calcRentalTypeAvailability</Name>
				<Source><![CDATA[
    /// <summary>
    /// Calculates the part availability from the EBCTmpBusinessObjects table records
    /// </summary>
    public void calcRentalTypeAvailability()
    {
        delete_from tmpData;
        delete_from tmpAssetExtraQtyLocal;

        DNRRFPTmpBusinessObjects tmpObjData;

        ttsbegin;

        delete_from tmpObjData;

        //Passing filters            
        businessObjectData.setRentalTypeIdata(rentalTypeIdsSelected);
        businessObjectData.parmInventLocationIdSetStr(FilterWarehouseId.text());
        businessObjectData.parmCalculationDateTime(calculationDateTime);

        //Calculation of availability data
        businessObjectData.setTmpData();

        TmpAssetAvailabilityTable.linkPhysicalTableInstance(businessObjectData.getTmpAvailabilityData());
        TmpAssetExtraQty.linkPhysicalTableInstance(businessObjectData.getTmpAssetExtraQty());

        TmpRentalTypeAvailability_ds.executeQuery();
        TmpAssetAvailabilityTable_ds.executeQuery();
        TmpAssetExtraQty_ds.executeQuery();

        ttscommit;
    }

]]></Source>
