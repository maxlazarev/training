			<Method>
				<Name>calcLineAmount</Name>
				<Source><![CDATA[
    public AmountCur calcLineAmount(Qty _qty = this.parmQty())
    {
        AmountCur ret;

        if (this.parmPrice())
        {
            ret = super(_qty);
        }
        else 
        {
            // Always force a calculation in case when Rental Price is 0.
            ret = this.calcPrice2LineAmount(_qty, true);
        }

        return ret;
    }

