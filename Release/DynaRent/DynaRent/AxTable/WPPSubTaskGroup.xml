    public static client server WPPSubTaskGroup find(WPPSubTaskGroupId _groupId, boolean _forUpdate = false)
    {
        WPPSubTaskGroup subTaskGroup;
    
        if (_groupId)
        {
            subTaskGroup.selectForUpdate(_forUpdate);
    
            select firstOnly subTaskGroup
            where subTaskGroup.GroupId == _groupId;
        }
    
        return subTaskGroup;
    }

