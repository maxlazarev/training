			<Method>
				<Name>subStatusJour</Name>
				<Source><![CDATA[
    VendRORSubStatusJour subStatusJour(boolean _update = false)
    {
        return VendRORSubStatusJour::findFromVendRORSubStatusTrans(this.PurchId, this.SubStatusId, this.SubStatusDate, _update);
    }

]]></Source>
			</Method>
			<Method>
				<Name>findRecId</Name>
				<Source><![CDATA[
    static CustRORSubStatusTrans findRecId(recId       recId,
                                      boolean     update = false )
    {
        CustRORSubStatusTrans subStatusTrans;
    
        subStatusTrans.selectForUpdate(update);
    
        select subStatusTrans
        where subStatusTrans.RecId== recId;
    
        return subStatusTrans;
    }

]]></Source>
