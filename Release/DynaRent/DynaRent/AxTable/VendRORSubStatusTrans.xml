			<Method>
				<Name>purchUnitTxt</Name>
				<Source><![CDATA[
    display UnitOfMeasureReportingText purchUnitTxt()
    {
        VendRORSubStatusJour subStatusJour = this.subStatusJour();
    
        if (subStatusJour)
        {
            return UnitOfMeasureReportingTranslation::unitOfMeasureReportingText(
                UnitOfMeasure::unitOfMeasureIdBySymbol(this.PurchUnit),
                subStatusJour.LanguageId);
        }
    
        if (this.PurchId)
        {
            return  UnitOfMeasureReportingTranslation::unitOfMeasureReportingText(
                UnitOfMeasure::unitOfMeasureIdBySymbol(this.PurchUnit),
                PurchTable::find(this.PurchId).LanguageId);
        }
    
        return  UnitOfMeasureReportingTranslation::unitOfMeasureReportingText(
            UnitOfMeasure::unitOfMeasureIdBySymbol(this.PurchUnit),
            CompanyInfo::languageId());
    }

]]></Source>
			</Method>
