			<Method>
				<Name>getHeader</Name>
				<Source><![CDATA[
    public RORRentalHeader getHeader(boolean _forUpdate = false)
    {
        return this.subStatusJour(_forUpdate);
    }

]]></Source>
			</Method>
			<Method>
				<Name>getParentLine</Name>
				<Source><![CDATA[
    public RORRentalLine getParentLine(boolean _forUpdate = false)
    {
        VendRORSubStatusTrans subStatusTrans;
    
        if (this.ParentLineRecId)
        {
            subStatusTrans.selectForUpdate(_forUpdate);
    
            select firstonly subStatusTrans
            where subStatusTrans.SubStatusId == this.SubStatusId
            &&    subStatusTrans.Date        == this.Date
            &&    subStatusTrans.PurchId     == this.PurchId
            &&    subStatusTrans.RecId       == this.ParentLineRecId;
    
            return subStatusTrans;
        }
        else
        {
            return null;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initFromPurchLine</Name>
				<Source><![CDATA[
    void  initFromPurchLine(PurchLine _purchLine)
    {
        this.OrigPurchId        = _purchLine.PurchId;
        this.InventTransId      = _purchLine.InventTransId;
        this.InventDimId        = _purchLine.InventDimId;
        this.ItemId             = _purchLine.ItemId;
        this.StockedProduct     = _purchLine.StockedProduct;
        this.LineNumber         = _purchLine.LineNumber;
        this.ExternalItemId     = _purchLine.ExternalItemId;
        this.Name               = _purchLine.Name;
        this.TaxItemGroup       = _purchLine.TaxItemGroup;
        this.TaxGroup           = _purchLine.TaxGroup;
        this.CurrencyCode       = _purchLine.CurrencyCode;
        this.PurchUnit          = _purchLine.PurchUnit;
        this.PriceUnit          = _purchLine.PriceUnit;
        this.Qty                = _purchLine.PurchReceivedNow;
        this.PurchPrice         = _purchLine.PurchPrice;
        this.PurchMarkup        = _purchLine.PurchMarkup;
        this.DiscPercent        = _purchLine.discPercent();
        this.DiscAmount         = _purchLine.discAmount();
        this.LineDisc           = _purchLine.LineDisc;
        this.LinePercent        = _purchLine.LinePercent;
        this.MultiLnDisc        = _purchLine.MultiLnDisc;
        this.MultiLnPercent     = _purchLine.MultiLnPercent;
        this.DefaultDimension   = this.copyDimension(_purchLine.DefaultDimension);
        this.SubStatusDate      = _purchLine.deliveryDate();
        this.LineHeader         = _purchLine.LineHeader;
        this.tradeType          = _purchLine.RORTradeType;
    }

]]></Source>
