				<Source><![CDATA[
[Form]
public class DNRDateRangeDropDialog extends FormRun
{
    boolean isFromDate;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        DNRIDateIntervalFilterWorkspace worksplace = this.args().caller();

        FromDate.dateValue(worksplace.getFromDate());
        ToDate.dateValue(worksplace.getToDate());
    }

]]></Source>
			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        super();

        Object caller = this.args().caller();

        caller.applyFilterDateRange(FromDate.dateValue(), ToDate.dateValue());
    }

]]></Source>
			</Method>
