						<Source><![CDATA[
        public void clicked()
        {
            if (!ObjectMaintenancePlan.RecId)
            {
                if (element.args().dataset() == tableNum(BOBObjectTable))
                {
                    BOBObjectTable objectTable = element.args().record();

                    ObjectMaintenancePlan.ObjectId = objectTable.ObjectId;
                }
                else if (element.args().dataset() == tableNum(BOBObjectView))
                {
                    BOBObjectView objectView = element.args().record();

                    ObjectMaintenancePlan.ObjectId = objectView.ObjectId;
                }
                else if (element.args().dataset() == tableNum(BOBObjectViewV2))
                {
                    BOBObjectViewV2 objectViewV2 = element.args().record();

                    ObjectMaintenancePlan.ObjectId = objectViewV2.ObjectId;
                }

            }
            super();
        }

