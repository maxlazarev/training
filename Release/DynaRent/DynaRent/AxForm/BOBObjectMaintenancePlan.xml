				<Source><![CDATA[
[Form]
public class BOBObjectMaintenancePlan extends FormRun
{
    WPPWorkOrderTable   workOrderTable;
    BOBObjectId         objectId;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (element.args().dataset() == tableNum(WPPWorkOrderTable))
        {
            workOrderTable  = element.args().record();
            objectId        = workOrderTable.BOBObjectTable().ObjectId;
        }

        super();

        //Initialize splitter
        //verticalSplitter = new SysFormSplitter_X(VSplitter, GridContainer, element, 300);

        BOBAddObjectMaintenance.enabled(element.args().dataset() == tableNum(BOBObjectTable)    ||
                                        element.args().dataset() == tableNum(BOBObjectView)     ||
                                        element.args().dataset() == tableNum(BOBObjectViewV2));
    
        ObjectMaintenancePlan_ds.updateDesign();
    }

]]></Source>
			</Method>
			<Method>
				<Name>recurrence</Name>
				<Source><![CDATA[
    public SysRecurrenceData recurrence(SysRecurrenceData _con = conNull())
    {
        if(_con)
        {
            objectMaintenancePlan.RecurrenceData = _con;
            BOBMaintenanceManager::runFromCommon(objectMaintenancePlan);
            objectMaintenancePlan_ds.write();
        }
    
        return objectMaintenancePlan.RecurrenceData;
    }

]]></Source>
			</Method>
			<Method>
				<Name>recurrenceUnits</Name>
				<Source><![CDATA[
    public container recurrenceUnits()
    {
        return [ SysRecurrenceUnit::Day,
                 SysRecurrenceUnit::Week,
                 SysRecurrenceUnit::Month,
                 SysRecurrenceUnit::Year ];
    }

]]></Source>
			</Method>
			<Method>
				<Name>showNextMaintenance</Name>
				<Source><![CDATA[
    public void showNextMaintenance()
    {
        Counter         cnt;
        SysRecurrence   sysRecurrence;
        TransDate       transDate;
        ;
    
        if(objectMaintenancePlan.RecurrenceData == conNull())
        {
            info("@SYS70700");
        }
        else
        {
            setPrefix("@SYS71293");
    
            sysRecurrence = new SysRecurrence(element.recurrence());
            sysRecurrence.reset();
    
            for(cnt = 1; cnt <= 25 && sysRecurrence.moveNext(); cnt ++)
            {
                transDate = sysRecurrence.currentDay();
    
                if(transDate < ObjectMaintenancePlan.NextDate)
                {
                    cnt --;
                }
                else
                {
                    info(strFmt("%1:  %2", cnt, transDate));
                }
            }
        }
    }

]]></Source>
			</Method>
