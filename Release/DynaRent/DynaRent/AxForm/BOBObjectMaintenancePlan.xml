						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildDataSource    qbdsObjectMaintenancePlan;
            QueryBuildDataSource    qbdsDefaultMaintenancePlan;
            QueryBuildDataSource    qbdsMaintenanceLog;
            ;
        
            super();
        
            if (workOrderTable)
            {
                qbdsObjectMaintenancePlan = ObjectMaintenancePlan_ds.query().dataSourceTable(tableNum(BOBObjectMaintenancePlan));
        
                if (objectId)
                {
                    qbdsObjectMaintenancePlan.addRange(fieldNum(BOBObjectMaintenancePlan, ObjectId)).value(SysQuery::value(objectId));
                }
        
                qbdsDefaultMaintenancePlan = qbdsObjectMaintenancePlan.addDataSource(tableNum(BOBDefaultMaintenancePlan));
                qbdsDefaultMaintenancePlan.relations(true);
        
                qbdsMaintenanceLog = qbdsDefaultMaintenancePlan.addDataSource(tableNum(BOBMaintenanceLog));
                qbdsMaintenanceLog.relations(true);
                qbdsMaintenanceLog.addRange(fieldNum(BOBMaintenanceLog, WPPWorkOrderId)).value(SysQuery::value(workOrderTable.WorkOrderId));
            }
        
            this.cacheAddMethod(tableMethodStr(BOBObjectMaintenancePlan, displayOpenWorkOrder));
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
        
            //objectMaintenancePlan_ds.setCreationMethod();
            objectMaintenancePlan_ds.updateDesign();
        
            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>updateDesign</Name>
						<Source><![CDATA[
        public void updateDesign()
        {
            BOBMaintenancePlanType planType = objectMaintenancePlan.defaultMaintenancePlan().PlanType;
            ;
        
            this.object(fieldNum(BOBObjectMaintenancePlan, RecurrenceUnit)).allowEdit(planType == BOBMaintenancePlanType::ConsumptionBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            this.object(fieldNum(BOBObjectMaintenancePlan, RecurrenceValue)).allowEdit(planType == BOBMaintenancePlanType::ConsumptionBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            this.object(fieldNum(BOBObjectMaintenancePlan, MeterTableRecId)).allowEdit(planType == BOBMaintenancePlanType::ConsumptionBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            this.object(fieldNum(BOBObjectMaintenancePlan, NextConsumption)).allowEdit(planType == BOBMaintenancePlanType::ConsumptionBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            this.object(fieldNum(BOBObjectMaintenancePlan, NextDate)).allowEdit(planType == BOBMaintenancePlanType::TimeBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            this.object(fieldNum(BOBObjectMaintenancePlan, AdvanceMargin)).allowEdit(ObjectMaintenancePlan.CreationMethod == BOBObjectMaintenanceCreationMethod::AdvanceNextMaintenance);
            this.object(fieldNum(BOBObjectMaintenancePlan, AdvanceMargin)).mandatory(ObjectMaintenancePlan.CreationMethod == BOBObjectMaintenanceCreationMethod::AdvanceNextMaintenance);
            this.object(fieldNum(BOBObjectMaintenancePlan, WPPTemplateId)).allowEdit(ObjectMaintenancePlan.CreationMethod != BOBObjectMaintenanceCreationMethod::Never);
            this.object(fieldNum(BOBObjectMaintenancePlan, WPPTemplateId)).mandatory(ObjectMaintenancePlan.CreationMethod != BOBObjectMaintenanceCreationMethod::Never);
            this.object(fieldNum(BOBObjectMaintenancePlan, WPPMainTaskId)).allowEdit(ObjectMaintenancePlan.CreationMethod != BOBObjectMaintenanceCreationMethod::Never);
        
            ConsumptionBased_M_editReset_ConsumptionBased.allowEdit(planType == BOBMaintenancePlanType::ConsumptionBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            TimeBased_M_editReset_TimeBased.allowEdit(planType == BOBMaintenancePlanType::TimeBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased);
        
            SysRecurrenceRun.enabled(objectMaintenancePlan.RecId && (planType == BOBMaintenancePlanType::TimeBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased));
            ButtonNextDates.enabled(objectMaintenancePlan.RecId && objectMaintenancePlan.RecurrenceData && (planType == BOBMaintenancePlanType::TimeBased || planType == BOBMaintenancePlanType::TimeAndConsumptionBased));
        
            WPPWorkOrderCreateFromMaintenancePlan.enabled(!BOBMaintenanceLog::openLogExists(ObjectMaintenancePlan.ObjectId, ObjectMaintenancePlan.DefaultPlanRecId));
            BOBMaintenanceItemReq.enabled(ObjectMaintenancePlan.WPPExternalTaskId != '');
        
            Warnings_InternalNote.allowEdit(ObjectMaintenancePlan.RecId);
        }

]]></Source>
					</Method>
					<Method>
						<Name>setCreationMethod</Name>
						<Source><![CDATA[
        public void setCreationMethod()
        {
            BOBObjectMaintenanceCreationMethod  creationMethod;
        
            if (ObjectMaintenancePlan.RecId)
            {
                if (ObjectMaintenancePlan.WPPTemplateId)
                {
                    creationMethod = ObjectMaintenancePlan.AdvanceMargin ?
                                        BOBObjectMaintenanceCreationMethod::AdvanceNextMaintenance :
                                        BOBObjectMaintenanceCreationMethod::NextMaintenance;
                }
                else
                {
                    creationMethod = BOBObjectMaintenanceCreationMethod::Never;
                }
        
                ObjectMaintenancePlan.CreationMethod = creationMethod;
                ObjectMaintenancePlan.update();
        
                ObjectMaintenancePlan_ds.refresh();
        
            }
            else
            {
                ObjectMaintenancePlan.CreationMethod = BOBObjectMaintenanceCreationMethod::NextMaintenance;
            }
        }

]]></Source>
