    public void init()
    {
       
         #resAppl
            
        sysListPanel = SysListPanelRelationTable::newForm(element,
                                                          element.controlId(formControlStr(DFMFieldGroup, TabPageContents)),
                                                          "@SYS27563",
                                                          "@DFM72",
                                                          #ImageField,
                                                          tableNum(DFMFieldGroupContents),
                                                          fieldNum(DFMFieldGroupContents, FieldId),
                                                          fieldNum(DFMFieldGroupContents, FieldGroupId),
                                                          tableNum(DFMFieldTable),
                                                          fieldNum(DFMFieldTable, FieldId),
                                                          [ fieldNum(DFMFieldTable, FieldId), fieldNum(DFMFieldTable, Name) ],
                                                          0,
                                                          '',
                                                          '',
                                                          0,
                                                          true,
                                                          true);        
        
        super();
        
        sysListPanel.init();
    }

]]></Source>
