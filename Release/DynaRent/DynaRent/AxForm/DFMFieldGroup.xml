				<Source><![CDATA[
    public void fillListPanel()
    {
        #DNRConstants

        if(FieldGroup.GroupId != sysListPanel.parmRelationRangeValue())
        {
            sysListPanel.parmRelationRangeValue(FieldGroup.RecId ? FieldGroup.GroupId : #DNRvalueNoMatch);
            sysListPanel.fill();
        }
    }

]]></Source>
