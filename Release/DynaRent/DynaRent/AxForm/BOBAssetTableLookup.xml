				<Source><![CDATA[
[Form]
public class BOBAssetTableLookup extends FormRun
{
    ItemId              itemIdFilter;
    Map                 tab2ComboBoxItemMap;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    [SuppressBPWarningAttribute("BPCheckInvalidInitFormMethodWarning", "Initialization must be performed before invoking super()")]
    public void init()
    {
        SalesLine salesLine;
        ;
    
        switch(element.args().dataset())
        {
            case tableNum(SalesLine):
                salesLine = element.args().record();
                itemIdFilter = salesLine.ItemId;
                break;
    
            default:
                throw error(Error::missingRecord(element.name()));
        }
    
        super();

        tab2ComboBoxItemMap = SysLookup::tab2ComboBox(CtrlTab, switchView);

        element.selectMode(AssetTable_All_AssetId);
    }

]]></Source>
