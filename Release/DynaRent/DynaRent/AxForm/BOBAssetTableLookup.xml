			<Control>
				<Name>CtrlTab</Name>
				<Type>Tab</Type>
				<Methods>
					<Method>
						<Name>tabChanged</Name>
						<Source><![CDATA[
        public void tabChanged(int _FromTab, int _ToTab)
        {
            #define.TabPageAll(1)
            #define.TabPageItem(2)

            super(_FromTab, _ToTab);

            switch (ctrlTab.tab())
            {
                case #TabPageAll:
                    element.selectMode(AssetTable_All_AssetId);
                    AssetTable_All_AssetId.setFocus();
                    break;

                case #TabPageItem:
                    element.selectMode(AssetTable_Item_AssetId);
                    AssetTable_Item_AssetId.setFocus();
                    break;
           
                default:
                    throw error(Error::wrongUseOfFunction(funcName()));
            }

        }

]]></Source>
					</Method>
