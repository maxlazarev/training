						<Source><![CDATA[
        public void init()
        {
            super();

            if(itemIdFilter)
            {
                QueryBuildDataSource qbdsObjectTable = this.queryBuildDataSource();
                qbdsObjectTable.addRange(fieldNum(BOBObjectTable, RentalTypeId)).value(BOBRentalType::findFromItemId(itemIdFilter).RentalTypeId);
            }
        }

