							<Method>
								<Name>lookup</Name>
								<Source><![CDATA[
            public void lookup(FormControl _formControl, str _filterStr)
            {
                ImageResourceName imgResName = new WPPPlanboardIcons().lookupImageResource(_formControl, _filterStr);

                if (CellIcon.ResourceId != imgResName)
                {
                    CellIcon.ResourceId = imgResName;
                    CellIcon_ds.write();

                    CellIcon_ds.refresh();
                }
            }

