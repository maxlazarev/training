						<Source><![CDATA[
        public void lookup()
        {
            int color = ColorSelection::selectColor(this, CellDataElement.Color);

            if (color != CellDataElement.Color)
            {
                CellDataElement.Color = color;
                CellDataElement_ds.write();

                CellDataElement_ds.clearDisplayOption(CellDataElement);
                CellDataElement_ds.refresh();
            }
        }

]]></Source>
