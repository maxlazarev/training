			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        recallOnly = (element.args().parmEnumType() == enumNum(BOBMaintenancePlanType) &&
                      element.args().parmEnum() == BOBMaintenancePlanType::Recall);
    
        super();
    
        //Initialize splitter
    
        if(recallOnly)
        {
            this.design().caption("@DNR7676");
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>recurrence</Name>
				<Source><![CDATA[
    public SysRecurrenceData recurrence(SysRecurrenceData _con = conNull())
    {
        if(_con)
        {
            defaultMaintenancePlan.RecurrenceData = _con;
            defaultMaintenancePlan_ds.write();
        }
    
        return defaultMaintenancePlan.RecurrenceData;
    }

]]></Source>
			</Method>
