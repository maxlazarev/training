    }

]]></Source>
			</Method>
			<Method>
				<Name>viewBusinessObjects</Name>
				<Source><![CDATA[
    public void viewBusinessObjects()
    {
        Query                       query;
        QueryRun                    queryRun;
        QueryBuildDataSource        qbds;
        BOBObjectTable              objectTable;
        ;
    
        if(DefaultMaintenancePlan.PlanType == BOBMaintenancePlanType::Recall)
        {
            if(DefaultMaintenancePlan.RecallQuery)
            {
                query = new Query(DefaultMaintenancePlan.RecallQuery);
    
                qbds = query.dataSourceTable(tableNum(BOBObjectTable));
                qbds.addSelectionField(fieldNum(BOBObjectTable, ObjectId));
                qbds.addSelectionField(fieldNum(BOBObjectTable, InventSerialId));
    
                queryRun = new QueryRun(query);
    
                setPrefix(strFmt("@DNR7075", SysQuery::countTotal(queryRun)));
    
                while(queryRun.next())
                {
                    objectTable = queryRun.get(tableNum(BOBObjectTable));
    
                    info(strFmt("%1: %2, %3: %4, %5: %6",
                                "@DNR5863", objectTable.ObjectId,
                                "@SYS477", objectTable.InventSerialId,
                                "@SYS25653", objectTable.assetId()));
                }
            }
            else
            {
                select count(RecId) from objectTable;
                setPrefix(strFmt("@DNR7075", objectTable.RecId));
                info("@DNR5877");
            }
        }
    }

]]></Source>
