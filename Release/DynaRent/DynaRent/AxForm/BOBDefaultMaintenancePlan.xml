						<Source><![CDATA[
        public void init()
        {
            QueryBuildRange qbr;
            ;
        
            super();
        
            if(recallOnly)
            {
                qbr = this.queryBuildDataSource().addRange(fieldNum(BOBDefaultMaintenancePlan, PlanType));
                qbr.value(SysQuery::value(BOBMaintenancePlanType::Recall));
                qbr.status(RangeStatus::Hidden);
        
                this.object(fieldNum(BOBDefaultMaintenancePlan, PlanType)).allowEdit(false);
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
        
            this.updateDesign();
        
            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>updateDesign</Name>
						<Source><![CDATA[
        public void updateDesign()
        {
            boolean isRecall            = defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::Recall;
            boolean allowEditRecall     = (isRecall && defaultMaintenancePlan.ConfirmedDateTime == utcDateTimeNull());
            boolean creationMethodNever = defaultMaintenancePlan.CreationMethod == BOBObjectMaintenanceCreationMethod::Never;
            ;
        
            this.object(fieldNum(BOBDefaultMaintenancePlan, CreationMethod)).allowEdit(!isRecall);
            this.object(fieldNum(BOBDefaultMaintenancePlan, AdvanceMargin)).allowEdit(!isRecall && defaultMaintenancePlan.CreationMethod == BOBObjectMaintenanceCreationMethod::AdvanceNextMaintenance);
            this.object(fieldNum(BOBDefaultMaintenancePlan, AdvanceMargin)).mandatory(!isRecall && defaultMaintenancePlan.CreationMethod == BOBObjectMaintenanceCreationMethod::AdvanceNextMaintenance);
            this.object(fieldNum(BOBDefaultMaintenancePlan, WPPTemplateId)).mandatory(!creationMethodNever);
            // always allow edit TemplateId even if creation method is Never - the new method can be derived from template
            //this.object(fieldNum(BOBDefaultMaintenancePlan, WPPTemplateId)).allowEdit(!creationMethodNever);
            this.object(fieldNum(BOBDefaultMaintenancePlan, WPPMainTaskId)).allowEdit(!isRecall && !creationMethodNever);
            this.object(fieldNum(BOBDefaultMaintenancePlan, WPPExternalTaskId)).allowEdit(!isRecall);
            this.object(fieldNum(BOBDefaultMaintenancePlan, PlanType)).allowEdit(!recallOnly && defaultMaintenancePlan.allowChangePlanType());
            this.object(fieldNum(BOBDefaultMaintenancePlan, RecurrenceUnit)).allowEdit(defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::ConsumptionBased || defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            this.object(fieldNum(BOBDefaultMaintenancePlan, RecurrenceValue)).allowEdit(defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::ConsumptionBased || defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            this.object(fieldNum(BOBDefaultMaintenancePlan, ReasonCode)).allowEdit(allowEditRecall);
            this.object(fieldNum(BOBDefaultMaintenancePlan, IsCritical)).allowEdit(allowEditRecall);
            this.object(fieldNum(BOBDefaultMaintenancePlan, StartDate)).allowEdit(allowEditRecall);
            this.object(fieldNum(BOBDefaultMaintenancePlan, EnableWarning)).allowEdit(!isRecall);
            this.object(fieldNum(BOBDefaultMaintenancePlan, ExceptEvery)).allowEdit(!isRecall);
        
            TabPageTimeBased.visible(!recallOnly);
            TabPageConsumptionBased.visible(!recallOnly);

            ConsumptionBased_M_editReset_ConsumptionBased.allowEdit(defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::ConsumptionBased || defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            TimeBased_M_editReset_TimeBased.allowEdit(defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::TimeBased || defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::TimeAndConsumptionBased);
        
            SysRecurrenceRun.enabled(defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::TimeBased || defaultMaintenancePlan.PlanType == BOBMaintenancePlanType::TimeAndConsumptionBased);
            buttonConfirm.enabled(allowEditRecall && defaultMaintenancePlan.displayHasQuery());
            buttonEditQuery.enabled(isRecall);
            buttonResetQuery.enabled(isRecall && !defaultMaintenancePlan.ConfirmedDateTime);
            ButtonViewBusinessObjects.enabled(isRecall && !defaultMaintenancePlan.ConfirmedDateTime);
        
            DefaultMaintenancePlan_ds.allowEdit(isRecall || defaultMaintenancePlan.ConfirmedDateTime == utcDateTimeNull());
            DefaultMaintenancePlan_ds.allowDelete(isRecall || defaultMaintenancePlan.ConfirmedDateTime == utcDateTimeNull());
        
            Warnings_InternalNote.allowEdit(!isRecall && DefaultMaintenancePlan.RecId);
        }

]]></Source>
