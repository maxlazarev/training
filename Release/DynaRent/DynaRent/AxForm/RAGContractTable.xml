					<Method>
						<Name>updateDesign</Name>
						<Source><![CDATA[
        public void updateDesign()
        {
            boolean         allowEditContract;
            boolean         isTypeOrder;
        
            RAGContractSubscription.enabled(ContractTable.ContractType == RAGContractType::VenueGroup ||
                                            ContractTable.ContractType == RAGContractType::Venue);
        
            // If this form was opened from another form, then for safety reasons you're only allowed to
            // edit/delete new contracts and contracts whose relation type corresponds to the calling record
            allowEditContract = (!isCallerLinkedTable || ContractTable.ContractType == contractDataParm.ContractType);
        
            ContractTable_ds.allowEdit(allowEditContract);
            ContractTable_ds.allowDelete(allowEditContract && ContractTable.Status == RAGContractStatus::Created);
        
            isTypeOrder = (ContractTable.ContractType == RAGContractType::RentalOrder ||
                           ContractTable.ContractType == RAGContractType::RentalOrderLine);
        
            ContractTable_ds.object(fieldNum(RAGContractTable, ReferenceModule)).allowEdit(isTypeOrder);
            ContractTable_ds.object(fieldNum(RAGContractTable, RentalSubRental)).allowEdit(!isTypeOrder);
            ContractTable_ds.object(fieldNum(RAGContractTable, RelationAccount)).allowEdit(ContractTable.ContractType != RAGContractType::Default);
        
            RAGChangeContractStatus.enabled(ContractTable.Status < RAGContractStatus::Canceled);
        }

]]></Source>
					</Method>
					<Method>
						<Name>initValue</Name>
						<Source><![CDATA[
        public void initValue()
        {
            CustTable           custTable;
            VendTable           vendTable;
            smmOpportunityTable smmOpportunityTable;
            boolean             hasDates;
            TransDate           fromDate;
            TransDate           toDate;
        
            super();
        
            // number sequence retrieval can't be done on the table since the
            // query's dynalink (if existing) will overwrite the sequence number
            ContractTable.ContractId = RAGContractTable::getNewContractId();
        
            if (isCallerLinkedTable)
            {
                ContractTable.ContractType      = contractDataParm.ContractType;
                ContractTable.RelationAccount   = contractDataParm.getRelationAccount();
                ContractTable.CurrencyCode      = contractDataParm.CurrencyCode;
                ContractTable.ReferenceModule   = contractDataParm.ContractRefModule;
                ContractTable.isActive          = NoYes::Yes;
                ContractTable.setRentalSubRentalFromModule();
        
                [hasDates, fromDate, toDate] = contractDataParm.getFromToDates();
                if (hasDates)
                {
                    ContractTable.FromDate = fromDate;
                    ContractTable.ToDate = toDate;
                }
            }
            else
            {
                ContractTable.ContractType = RAGParameters::tableId2ContractType(element.args().dataset());
        
                switch (element.args().dataset())
                {
                    case tableNum(CustTable):
                        custTable                     = element.args().record();
                        ContractTable.RelationAccount = smmBusRelTable::findCust(custTable.AccountNum).BusRelAccount;
                        break;
        
                    case tableNum(VendTable):
                        vendTable                     = element.args().record();
                        ContractTable.RelationAccount = smmBusRelTable::findVend(vendTable.AccountNum).BusRelAccount;
                        break;
        
                    case tableNum(smmOpportunityTable):
                        smmOpportunityTable = element.args().record();
        
                        if (DirPartyTableHelper::isRelation(smmOpportunityTable.Party)
                        ||  DirPartyTableHelper::isCustomer(smmOpportunityTable.Party)
                        ||  DirPartyTableHelper::isvendor(smmOpportunityTable.Party))
                        {
                            ContractTable.RelationAccount = smmBusRelTable::findByParty(smmOpportunityTable.Party).BusRelAccount;
                        }
                        break;
                }
            }
        }

]]></Source>
