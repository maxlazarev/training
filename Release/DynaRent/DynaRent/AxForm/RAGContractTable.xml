					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            #DNRConstants
            CustTable           custTable;
            VendTable           vendTable;
            smmOpportunityTable smmOpportunityTable;
            ProjTable           projTable;
            ProjGroup           projGroup;
        
            super();
        
            switch (element.args().dataset())
            {
                // in case we're opening this form from the list page's 'new contract' button,
                // we don't want the highlighted contract in the list page to appear here
                case tableNum(RAGContractView):
                    if (element.args().menuItemName() == menuitemDisplayStr(RAGContractTableNew))
                    {
                        this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, ContractId)).value(#DNRvalueNoMatch);
                    }
                    break;
        
                case tableNum(CustTable):
                    custTable = element.args().record();
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, RelationAccount)).value(smmBusRelTable::findCust(custTable.AccountNum).BusRelAccount);
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, ContractType)).value(SysQuery::value(RAGContractType::BusinessRelation));
                    break;
        
                case tableNum(VendTable):
                    vendTable = element.args().record();
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, RelationAccount)).value(smmBusRelTable::findVend(vendTable.AccountNum).BusRelAccount);
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, ContractType)).value(SysQuery::value(RAGContractType::BusinessRelation));
                    break;
        
                case tableNum(smmOpportunityTable):
                    smmOpportunityTable = element.args().record();
        
                    if (DirPartyTableHelper::isRelation(smmOpportunityTable.Party)
                    ||  DirPartyTableHelper::isCustomer(smmOpportunityTable.Party)
                    ||  DirPartyTableHelper::isvendor(smmOpportunityTable.Party))
                    {
                        this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, RelationAccount)).value(smmBusRelTable::findByParty(smmOpportunityTable.Party).BusRelAccount);
                        this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, ContractType)).value(SysQuery::value(RAGContractType::BusinessRelation));
                    }
                    else
                    {
                        throw error(strFmt("@DNR7489", smmOpportunityTable.OpportunityId));
                    }
                    break;

                case tableNum(ProjTable):
                    projTable = element.args().record();
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, RelationAccount)).value(projTable.projid);
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, ContractType)).value(SysQuery::value(RAGContractType::Project));
                    break;

                case tableNum(ProjGroup):
                    projGroup = element.args().record();
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, RelationAccount)).value(projGroup.ProjGroupId);
                    this.queryBuildDataSource().addRange(fieldNum(RAGContractTable, ContractType)).value(SysQuery::value(RAGContractType::ProjectGroup));
                    break;
            }
        }

]]></Source>
