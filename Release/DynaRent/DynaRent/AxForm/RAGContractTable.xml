			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
    
        if (element.args().parmEnumType() == enumNum(RAGContractStatus) && element.args().parmEnum() == RAGContractStatus::Confirmed)
        {
            if (!element.args().record()) // called from menu
            {
                QueryBuildRange queryRangeConfirmed = ContractTable_DS.query().dataSourceTable(tableNum(RAGContractTable)).addRange(fieldNum(RAGContractTable, Status));
                queryRangeConfirmed.value(queryValue(RAGContractStatus::Confirmed));
                queryRangeConfirmed.status(RangeStatus::Locked);
            }
        }
    
        // when this form is called from one of the "trade tables", filter and order
        // on the relevant contract hierarchy instead of using the regular dynalink
        if (element.shouldFilterOnHierarchy(element.args().dataset()))
        {
            isCallerLinkedTable = true;
            ContractTable_ds.executeQuery(); // load the data
    
            if (ContractTable.RecId)
            {
                contractDataParm.initFromContractTable(ContractTable);
            }
            else
            {
                // there's no specific contract linked to the calling record yet,
                // so use that record to build the hierarchy instead of a contract
                contractDataParm.initFromCommon(element.args().record());
            }
    
            ContractTable_ds.setContractHierarchyQuery();
            ContractTable_ds.executeQuery(); // apply the new query
        }
    }

]]></Source>
