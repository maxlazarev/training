					<Method>
						<Name>setContractHierarchyQuery</Name>
						<Source><![CDATA[
        public void setContractHierarchyQuery()
        {
            Query                   query;
            QueryBuildDataSource    qbdsContractTable;
            QueryBuildDataSource    qbdsPriority;
        
            query = new Query();
        
            qbdsContractTable = query.addDataSource(tableNum(RAGContractTable));
            qbdsContractTable.addRange(fieldNum(RAGContractTable, ContractId)).value(contractDataParm.contractHierarchyRange());
        
            qbdsPriority = qbdsContractTable.addDataSource(tableNum(RAGContractTypePriority));
            qbdsPriority.addLink(fieldNum(RAGContractTable, ContractType), fieldNum(RAGContractTypePriority, ContractType));
            qbdsPriority.addSelectionField(fieldNum(RAGContractTypePriority, TableId));
            qbdsPriority.addSortField(fieldNum(RAGContractTypePriority, Priority), SortOrder::Descending);
        
            ContractTable_ds.query(query);
        }

]]></Source>
