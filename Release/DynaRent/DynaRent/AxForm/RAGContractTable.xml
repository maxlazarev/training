			<Method>
				<Name>shouldFilterOnHierarchy</Name>
				<Source><![CDATA[
    private boolean shouldFilterOnHierarchy(TableId _tableId)
    {
        boolean ret = false;
    
        if (_tableId == tableNum(SalesTable)
        ||  _tableId == tableNum(PurchTable)
        ||  _tableId == tableNum(SalesQuotationTable)
        ||  _tableId == tableNum(SalesLine)
        ||  _tableId == tableNum(PurchLine)
        ||  _tableId == tableNum(SalesQuotationLine))
        {
            ret = true;
        }
    
        return ret;
    }

]]></Source>
