				<Source><![CDATA[
[Form]
public class WPPResourceIdLookUp extends FormRun
{
    WPPResourceCategory     resourceCategory;
    boolean                 useResourceCategory;
    boolean                 onlyIndividualResource;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        //The selected ResourceId will be returned
        element.selectMode(ResourceTable_ResourceId);

    }

]]></Source>
			</Method>
			<Method>
				<Name>parmOnlyIndividualResource</Name>
				<Source><![CDATA[
    void  parmOnlyIndividualResource(boolean _onlyIndividualResource)
    {
        onlyIndividualResource = _onlyIndividualResource;
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmResourceCategory</Name>
				<Source><![CDATA[
    void  parmResourceCategory(boolean _useResourceCategory, WPPResourceCategory _resourceCategory)
    {
        useResourceCategory = _useResourceCategory;
        resourceCategory = _resourceCategory;
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        FormStringControl   callerControl   = SysTableLookup::getCallerStringControl(element.args());
        boolean             filterLookup    = false;
        
        filterLookup = SysTableLookup::filterLookupPreRun(
            callerControl,
            ResourceTable_ResourceId,
            resourceTable_ds);
        
        super();
        
        SysTableLookup::filterLookupPostRun(
            filterLookup,
            callerControl.text(),
            ResourceTable_ResourceId,
            resourceTable_ds);
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>ResourceTable</Name>
				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            QueryBuildDataSource qbdsResourceTable = this.queryBuildDataSource();

            if (useResourceCategory)
            {
                qbdsResourceTable.addRange(fieldnum(WPPResourceTable, ResourceCategory)).value(queryValue(resourceCategory));
            }

            if(onlyIndividualResource)
            {
                qbdsResourceTable.addRange(fieldnum(WPPResourceTable, IsIndividualResource)).value(queryValue(NoYes::Yes));
            }
        }

]]></Source>
					</Method>
				</Methods>
