						<Source><![CDATA[
        public void linkActive()
        {
            QueryBuildDataSource    qbds;
            Object                  vendEditInvoice, vendInvoiceInfoListPage;
        
            VendinvoiceInfoTable    vendinvoiceInfoTableLocal;
        
            super();
        
            if (callerForm.name() == formStr(VendEditInvoice))
            {
                vendEditInvoice = callerForm;
                vendinvoiceInfoTableLocal = vendEditInvoice.getVendInvoiceInfoTable_RU();
            }
            else
            {
                vendinvoiceInfoTableLocal = element.args().record();
            }
        
            qbds = LedgerJournalTrans_q.dataSourceTable(tableNum(LedgerJournalTrans));
        
            SysQuery::findOrCreateRange(qbds, fieldNum(LedgerJournalTrans, RecId)).value(SysQuery::value(vendinvoiceInfoTableLocal.PoolRecId));
        
        
            LedgerJournalTrans_ds.executeQuery();
            LedgerJournalTrans_ds.reread();
            LedgerJournalTrans_ds.refresh();
        }

]]></Source>
