				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class FINAutoSettlement extends FormRun
{
    FINAutoSettlement  autoSettlement;

}
]]></Source>
			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        boolean ret = true;
    
        autoSettlement.setValues(FINtmpAutoSettlement);
    
        if (!autoSettlement.parmLedgerAccount())
        {
            ret = checkFailed(strFmt("@SYS26332", "@SYS8343"));
        }
    
        if ((autoSettlement.parmToDate()) && (autoSettlement.parmToDate() < autoSettlement.parmFromDate()))
        {
            ret = checkFailed(strFmt("@SYS71086", "@SYS35904", "@SYS24050"));
        }
    
        if (ret)
        {
            super();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>enableControls</Name>
				<Source><![CDATA[
    public void enableControls()
    {
        DimensionAttribute.enabled(SettleMethod.selection() == FINSettleMethodEnum::Dimension);
    
        if (DimensionAttribute.enabled())
        {
            FINtmpAutoSettlement.DimensionAttribute = autoSettlement.parmDimension();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    [SuppressBPWarningAttribute("BPCheckInvalidInitFormMethodWarning", "Validation of caller must be performed before invoking super()")]
    public void init()
    {
    
        if (!element.args() || !element.args().caller())
        {
            throw error(strFmt("@SYS22862", element.name()));
        }
    
        if (classIdGet(element.args().caller()) != classIdGet(autoSettlement))
        {
            throw error(strFmt("@SYS22456", element.name(), classId2Name(classIdGet(autoSettlement))));
        }
    
        autoSettlement = element.args().caller();
    
        super();
    
        element.enableControls();
    }

]]></Source>
