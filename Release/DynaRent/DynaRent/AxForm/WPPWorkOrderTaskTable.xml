					<Method>
						<Name>updateDesign</Name>
						<Source><![CDATA[
        public void updateDesign()
        {
            boolean autoUpdateEnabled = WPPWorkOrderTaskTable.AutoPostUpdateDocuments;
            boolean synchronizeRentalTrans = WPPWorkOrderTaskTable.SynchronizeRentalTransaction;
        
            WPPWorkOrderStatusPostingSetup_ds.allowCreate(autoUpdateEnabled || synchronizeRentalTrans);
            WPPWorkOrderStatusPostingSetup_ds.allowEdit(autoUpdateEnabled || synchronizeRentalTrans);
        
            NewPostingStatusSetup.enabled(WPPWorkOrderTaskTable.RecId);
        
            WPPWorkOrderStatusPostingSetup_ds.object(fieldNum(WPPWorkOrderStatusPostingSetup, AutomaticPosting)).allowEdit(WPPWorkOrderTaskTable.AutoPostUpdateDocuments &&
                                                                                                                               WPPWorkOrderStatusPostingSetup.BOBRentalSubStatus != BOBRentalSubStatus::Picked);
            WPPWorkOrderStatusPostingSetup_ds.object(fieldNum(WPPWorkOrderStatusPostingSetup, ShowAllReturnLines)).allowEdit(WPPWorkOrderTaskTable.AutoPostUpdateDocuments &&
                                                                                                                             WPPWorkOrderStatusPostingSetup.BOBRentalSubStatus == BOBRentalSubStatus::Returned);
        
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, SetObjectAvailStatus)).allowEdit(WPPWorkOrderStatusPostingSetup.BOBRentalSubStatus != BOBRentalSubStatus::Picked);
        }

]]></Source>
