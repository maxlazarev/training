        public void updateDesign()
        {
            boolean allowEdit = (WPPWorkOrderTaskTable.TaskCategory != WPPTaskCategory::InternalTask);
        
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, AutoPostType)).allowEdit(WPPWorkOrderTaskTable.AutoPostUpdateDocuments);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, BOBObjectAssignment)).allowEdit(WPPWorkOrderTaskTable.TaskType == WPPTaskType::Service);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, AutoScheduleLinkedResources)).allowEdit(WPPWorkOrderTaskTable.TaskType == WPPTaskType::Service);
        
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, TaskGroupId)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, SetObjectNotAvailStatus)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, SetObjectAvailStatus)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, BOBObjectAssignment)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, AutoScheduleLinkedResources)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, SyncResourceQty)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, AutoGenerateJourTrans)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, AutoPostUpdateDocuments)).allowEdit(allowEdit);
            WPPWorkOrderTaskTable_ds.object(fieldNum(WPPWorkOrderTaskTable, SynchronizeRentalTransaction)).allowEdit(allowEdit);
        }

]]></Source>
