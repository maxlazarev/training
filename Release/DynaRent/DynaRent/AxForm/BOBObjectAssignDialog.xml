			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    [SuppressBPWarningAttribute("BPCheckInvalidInitFormMethodWarning", "Validation of caller must be performed before invoking super()")]
    public void init()
    {
        if(element.args().caller())
        {
            DialogRunbase caller = element.args().caller();
            engine = caller.runBase();
            callerRentalLine = engine.parmRentalLine();

            if(callerRentalLine.TradeType == RORTradeType::SalesPurch)
            {
                throw error(strFmt("@DNR8247", element.name(), "@SYS12223", "@DNR1720"));
            }
        }
        else
        {
            throw error(error::missingRecord(element.name()));
        }

        parameters = BOBParameters::find();
        userTimeZone = DateTimeUtil::getUserPreferredTimeZone();

        dataClass = new BOBObjectAvailabilityData();
        dataClass.parmCallerRentalLine(callerRentalLine);

        element.updateAvailabilitySettings();

       super(); 

        element.linkPhysicalTableInstance();
    }

]]></Source>
			</Method>
