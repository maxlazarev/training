			<Method>
				<Name>createSubRentalOrder</Name>
				<Source><![CDATA[
    private void createSubRentalOrder(BOBObjectAssignmentEngine _engine)
    {
        SalesLine salesLine = SalesLine::findRecId(callerRentalLine.RecId);
        SalesTable salesTable = salesLine.salesTable();

        if (!salesLine
        ||  (salesLine.RORTradeType == RORTradeType::Rental     &&
             salesLine.SalesType == SalesType::RORRental        &&
             salesLine.SalesStatus == SalesStatus::Backorder    &&
             salesLine.RemainInventPhysical != 0                &&
             salesTable.FINCreditCheckNoWarning(true, false)    &&
             salesTable.type().canCreatePurchOrder()            &&
             // line should not have object assigned yet
             !salesLine.inventDim().inventSerialId              &&
             salesLine.type().RORcanCreatePurchOrder()))
        {
            Args args = new Args();
            // Button is only available from Rental order lines
            // so callerRentalLine is always a SalesLine
            args.record(callerRentalLine);

            MenuFunction mf = new MenuFunction(menuitemActionStr(PurchCreateFromSalesOrder_RORFromLine), MenuItemType::Action);
            mf.run(args);

            salesLine = SalesLine::findRecId(callerRentalLine.RecId);
            PurchLine purchLine = purchLine::findInventTransId(salesLine.InventRefTransId);
            InventDim inventDimPurchLine = purchLine.inventDim();

            // Only close the form when a sub rental order has been created
            if (_engine
            &&  inventDimPurchLine.inventSerialId
            &&  inventDimPurchLine.InventSerialId == salesLine.inventDim().InventSerialId)
            {
                _engine.parmWasSubRentalCreated(true);
                element.closeCancel();
            }
        }
        else
        {
            warning(strFmt("@DNR8597", salesLine.SalesId));
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateAvailabilitySettings</Name>
				<Source><![CDATA[
    private void updateAvailabilitySettings()
    {
        #TimeConstants

        RORRentalDateTimes              rentalDateTimes;
        BOBObjectAvailabilitySettings   settingsLocal;
        Common                          common = callerRentalLine;
        InventTransferLine              inventTransferLine = common as InventTransferLine;
        BOBRentalType                   rentalType;
        ;

        ttsBegin;

        if(callerRentalLine)
        {
            rentalType = BOBRentalType::find(callerRentalLine.BOBRentalTypeId);
        }

        settingsLocal = BOBObjectAvailabilitySettings::findOrCreate(curUserId(), true, true);

        if (parameters.AssignmentFilterRentalTypeId && callerRentalLine)
        {
            settingsLocal.RentalTypeId = callerRentalLine.BOBRentalTypeId;
        }
        else if(!parameters.AssignmentFilterRentalTypeId)
        {
            settingsLocal.RentalTypeId = '';
        }

        if(parameters.AssignmentFilterObjectGroupId && callerRentalLine)
        {
            settingsLocal.ObjectGroupId = rentalType.ObjectGroupId;
        }
        else if(!parameters.AssignmentFilterObjectGroupId)
        {
            settingsLocal.ObjectGroupId = '';
        }

        if(parameters.AssignmentFilterObjectSubGroupId && callerRentalLine)
        {
            settingsLocal.ObjectSubGroupId = rentalType.ObjectSubGroupId;
        }
        else if(!parameters.AssignmentFilterObjectSubGroupId)
        {
            settingsLocal.ObjectSubGroupId = '';
        }

        if(parameters.AssignmentFilterCurrentDepotOnly)
        {
            settingsLocal.Depot = (callerRentalLine ?
                                   InventDim::find(callerRentalLine.InventDimId).InventLocationId :
                                   DNRWorkerDefaults::findByUserId().InventLocationId);
        }
        else
        {
            settingsLocal.Depot = '';
        }

        settingsLocal.Available         = parameters.AssignmentFilterBusObjAvailableOnly;
        settingsLocal.CurrentCompany    = parameters.AssignmentFilterCurrentCompanyOnly;
        settingsLocal.CurrentObjects    = parameters.AssignmentFilterCurrentObjectsOnly;
        settingsLocal.BusObjAttrs       = parameters.AssignmentFilterBusObjAttrs && element.args().caller();

        if(inventTransferLine)
        {
            settingsLocal.StartDateTime = inventTransferLine.shipTransDateTime();
            settingsLocal.EndDateTime   = inventTransferLine.receiveTransDateTime();
        }
        else if(callerRentalLine)
        {
            rentalDateTimes = (callerRentalLine.RecId ?
                               callerRentalLine.RORRentalDateTimesHost::rentalDateTimes() :
                               callerRentalLine.RORGetHeader().RORRentalDateTimesHost::rentalDateTimes());

            settingsLocal.StartDateTime = rentalDateTimes.OnRentDateTime;
            settingsLocal.EndDateTime   = (DateTimeUtil::DNRIsDateTimePopulated(rentalDateTimes.OffRentDateTime) ?
                                            rentalDateTimes.OffRentDateTime : rentalDateTimes.ExpectedOffRentDateTime);


            settingsLocal.WPPDisplayMode = WPPPlanboardDisplayMode::Grid;
        }
        else if(!settingsLocal.StartDateTime || !settingsLocal.EndDateTime)
        {
            settingsLocal.StartDateTime   = DateTimeUtil::newDateTime((DateTimeUtil::getSystemDate(userTimeZone) - #daysPerWeek), 0, userTimeZone);
            settingsLocal.EndDateTime     = DateTimeUtil::newDateTime((DateTimeUtil::getSystemDate(userTimeZone) + #daysPerWeek), timeMax(), userTimeZone);
        }

        // Graphical availability options
        settingsLocal.RentalBusinessObject        = parameters.AssignmentFilterInclRentalBusObject;
        settingsLocal.SubRentalBusinessObject     = parameters.AssignmentFilterInclSubRentalBusObject;
        settingsLocal.ExternalBusinessObject      = parameters.AssignmentFilterInclExternalBusObject;
        settingsLocal.SerializedBusinessObject    = parameters.AssignmentFilterInclSerializedBusObj;
        settingsLocal.BulkBusinessObject          = parameters.AssignmentFilterInclBulkBusObject;

        settingsLocal.update();

        ttsCommit;
    }

]]></Source>
